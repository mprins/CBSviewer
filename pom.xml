<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>nl.cbs.cbsviewer</groupId>
	<artifactId>cbsviewer</artifactId>
	<packaging>war</packaging>
	<version>1.2-SNAPSHOT</version>
	<name>CBSviewer Webapp</name>
	<description>Webrichtlijnen compliant webmap viewer.</description>
	<url>http://mineleni.github.io/CBSviewer</url>
	<inceptionYear>2012</inceptionYear>
	<organization>
		<name><![CDATA[Ministerie EZ]]></name>
		<url>https://github.com/organizations/MinELenI</url>
	</organization>
	<developers>
		<developer>
			<id>mprins</id>
			<name>Mark Prins</name>
			<url>https://github.com/mprins</url>
			<organization>Dienst Landelijk Gebied, Ministerie EZ</organization>
			<organizationUrl>http://www.dienstlandelijkgebied.nl/</organizationUrl>
		</developer>
		<developer>
			<id>pmanders</id>
			<name>Paul Manders</name>
			<organization>Centraal Bureau voor de Statistiek, Ministerie EZ</organization>
			<organizationUrl>http://www.cbs.nl/</organizationUrl>
		</developer>
	</developers>
	<contributors />
	<licenses>
		<license>
			<name>BSD license</name>
			<url>https://raw.github.com/MinELenI/CBSviewer/master/LICENSE.md</url>
			<comments />
			<distribution>repo</distribution>
		</license>
		<license>
			<name>BSD license</name>
			<url>https://raw.github.com/openlayers/openlayers/master/license.txt</url>
			<comments>For any parts of OpenLayers</comments>
			<distribution>manual</distribution>
		</license>
		<license>
			<name>MIT license</name>
			<url>https://raw.github.com/jquery/jquery/master/MIT-LICENSE.txt</url>
			<comments>For any parts of jQuery</comments>
			<distribution>repo</distribution>
		</license>
		<license>
			<name>MIT license</name>
			<url>https://raw.github.com/jquery/jquery-ui/master/MIT-LICENSE.txt</url>
			<comments>For any parts of jQuery-UI</comments>
			<distribution>repo</distribution>
		</license>
		<license>
			<name>MIT license</name>
			<url>https://raw.github.com/jhy/jsoup/master/LICENSE</url>
			<comments>For any parts of jsoup</comments>
			<distribution>repo</distribution>
		</license>
		<license>
			<name>GNU Lesser General Public License (LGPL)</name>
			<comments>The binary distribution of this project includes part of Geotools.</comments>
			<distribution>repo</distribution>
			<url>https://raw.github.com/mprins/geotools/master/licenses/LICENSE</url>
		</license>
	</licenses>
	<issueManagement>
		<system>GitHub</system>
		<url>https://github.com/MinELenI/CBSviewer/issues/</url>
	</issueManagement>
	<scm>
		<url>https://github.com/MinELenI/CBSviewer</url>
		<connection>scm:git:git@github.com:git://github.com/MinELenI/CBSviewer.git</connection>
		<developerConnection>scm:git:git@github.com:git@github.com:MinELenI/CBSviewer.git</developerConnection>
		<tag>master</tag>
	</scm>
	<ciManagement>
		<system>Travis-CI</system>
		<url>https://travis-ci.org/MinELenI/CBSviewer/</url>
		<notifiers />
	</ciManagement>
	<prerequisites>
		<maven>3.2</maven>
	</prerequisites>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<geotools.version>12.3</geotools.version>
		<httpcomponents.version>4.4.1</httpcomponents.version>
		<jaxb.version>2.2.12</jaxb.version>
		<proj.finalname>${project.artifactId}</proj.finalname>
		<jdk.version>1.7</jdk.version>
		<phantomjs.version>1.9.8</phantomjs.version>
		<github.username />
		<github.password />
		<skipTests>false</skipTests>
		<github.site.dryRun>false</github.site.dryRun>
		<http.proxyHost />
		<http.proxyPort>0</http.proxyPort>
		<debug>false</debug>
		<sass.OutputStyle>:compressed</sass.OutputStyle>
		<openlayers.checkout.url>scm:git:https://github.com/openlayers/openlayers.git</openlayers.checkout.url>
		<!-- tag, branch, ... -->
		<!--
		<openlayers.checkout.versiontype>tag</openlayers.checkout.versiontype>
		<openlayers.checkout.version>release-2.13.1</openlayers.checkout.version>
		-->
		<openlayers.checkout.versiontype>branch</openlayers.checkout.versiontype>
		<openlayers.checkout.version>master</openlayers.checkout.version>
		<!-- this can be any other OL build compressor or 'none' -->
		<openlayers.compress.build>closure</openlayers.compress.build>
		<!-- Jetty 7.x is J5EE, 2.5 servlet api, 1.2 JSP api -->
		<jetty.version>7.6.16.v20140903</jetty.version>
		<!-- let op in een aantal integratie tests is deze poort hardcoded -->
		<jetty.port>8021</jetty.port>
		<jetty.stopPort>8007</jetty.stopPort>
		<repo_token />
		<surefire.argline>-XX:-UseSplitVerifier</surefire.argline>
	</properties>
	<profiles>
		<profile>
			<id>jdk8</id>
			<activation>
				<jdk>1.8</jdk>
			</activation>
			<properties>
				<jdk.version>1.8</jdk.version>
				<surefire.argline />
				<jdk8.javadoc.param>-Xdoclint:none</jdk8.javadoc.param>
			</properties>
		</profile>
		<profile>
			<id>developer</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<debug>true</debug>
				<sass.OutputStyle>:expanded</sass.OutputStyle>
			</properties>
			<build>
				<plugins>
					<plugin>
						<groupId>org.mortbay.jetty</groupId>
						<artifactId>jetty-maven-plugin</artifactId>
						<version>${jetty.version}</version>
						<configuration>
							<stopPort>${jetty.stopPort}</stopPort>
							<stopKey>STOP</stopKey>
							<connectors>
								<connector implementation="org.eclipse.jetty.server.bio.SocketConnector">
									<port>${jetty.port}</port>
									<maxIdleTime>60000</maxIdleTime>
								</connector>
							</connectors>
							<webAppConfig>
								<baseResource implementation="org.eclipse.jetty.util.resource.ResourceCollection">
									<resourcesAsCSV>src/main/webapp,target/cbsviewer</resourcesAsCSV>
								</baseResource>
								<sessionHandler implementation="org.eclipse.jetty.server.session.SessionHandler">
									<sessionManager implementation="org.eclipse.jetty.server.session.HashSessionManager">
										<usingCookies>false</usingCookies>
									</sessionManager>
								</sessionHandler>
							</webAppConfig>
							<reload>manual</reload>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<!-- specify this profile to build openlayers as part of the build, ie.
				-Popenlayers -->
			<id>openlayers</id>
			<properties>
			</properties>
			<build>
				<defaultGoal>process-test-classes</defaultGoal>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-scm-plugin</artifactId>
						<version>1.9.4</version>
						<executions>
							<execution>
								<id>getOpenLayers</id>
								<phase>initialize</phase>
								<goals>
									<goal>checkout</goal>
								</goals>
								<configuration>
									<connectionUrl>${openlayers.checkout.url}</connectionUrl>
									<checkoutDirectory>${project.build.directory}/OLcheckout</checkoutDirectory>
									<scmVersion>${openlayers.checkout.version}</scmVersion>
									<scmVersionType>${openlayers.checkout.versiontype}</scmVersionType>
								</configuration>
							</execution>
						</executions>
					</plugin>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>exec-maven-plugin</artifactId>
						<version>1.4.0</version>
						<executions>
							<execution>
								<id>buildOpenLayers</id>
								<goals>
									<goal>exec</goal>
								</goals>
								<phase>process-sources</phase>
								<configuration>
									<!-- note: a python executable needs to be in the path -->
									<executable>python</executable>
									<workingDirectory>${project.build.directory}/OLcheckout/build</workingDirectory>
									<arguments>
										<argument>build.py</argument>
										<argument>-c</argument>
										<argument>${openlayers.compress.build}</argument>
										<argument>${basedir}/OL-build.cfg</argument>
									</arguments>
								</configuration>
							</execution>
							<execution>
								<id>buildOpenLayers-test</id>
								<goals>
									<goal>exec</goal>
								</goals>
								<phase>generate-test-sources</phase>
								<configuration>
									<!-- note: a python executable needs to be in the path -->
									<executable>python</executable>
									<workingDirectory>${project.build.directory}/OLcheckout/build</workingDirectory>
									<arguments>
										<argument>buildUncompressed.py</argument>
										<argument>${basedir}/OL-build</argument>
										<argument>OpenLayers-test.js</argument>
									</arguments>
								</configuration>
							</execution>
						</executions>
					</plugin>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-antrun-plugin</artifactId>
						<version>1.8</version>
						<executions>
							<execution>
								<id>prepareOpenLayers</id>
								<phase>generate-sources</phase>
								<goals>
									<goal>run</goal>
								</goals>
								<configuration>
									<target>
										<setproxy proxyhost="${http.proxyHost}"
												  proxyport="${http.proxyPort}" />
										<mkdir dir="${basedir}/cache" />
										<get src="https://dl.google.com/closure-compiler/compiler-latest.zip"
											 dest="${basedir}/cache/"
											 usetimestamp="true" />
										<unzip src="${basedir}/cache/compiler-latest.zip"
											   dest="${basedir}/target">
											<patternset>
												<include name="**/compiler.jar" />
											</patternset>
										</unzip>
										<copy file="${basedir}/target/compiler.jar"
											  tofile="${project.build.directory}/OLcheckout/tools/closure-compiler.jar"
											  overwrite="true" />
									</target>
								</configuration>
							</execution>
							<execution>
								<id>copyOpenLayers</id>
								<phase>process-classes</phase>
								<goals>
									<goal>run</goal>
								</goals>
								<configuration>
									<!-- copy files into build -->
									<target>
										<copy file="${project.build.directory}/OLcheckout/build/OpenLayers.js"
											  tofile="src/main/webapp/lib/OpenLayers.js" />
									</target>
								</configuration>
							</execution>
							<execution>
								<id>copyOpenLayers-test</id>
								<phase>process-test-sources</phase>
								<goals>
									<goal>run</goal>
								</goals>
								<configuration>
									<target>
										<copy file="${project.build.directory}/OLcheckout/build/OpenLayers-test.js"
											  tofile="src/test/js/lib/OpenLayers-test.js" />
									</target>
								</configuration>
							</execution>
						</executions>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<id>jenkins</id>
			<properties>
				<github.global.server>github</github.global.server>
			</properties>
			<build>
				<plugins>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>cobertura-maven-plugin</artifactId>
						<version>2.7</version>
						<configuration>
							<format>xml</format>
						</configuration>
					</plugin>
					<plugin>
						<groupId>org.eluder.coveralls</groupId>
						<artifactId>coveralls-maven-plugin</artifactId>
						<version>3.1.0</version>
						<configuration>
							<repoToken>${repo_token}</repoToken>
							<sourceDirectories>
								<source>src/main/java</source>
								<source>target/generated-sources/jaxb</source>
							</sourceDirectories>
							<sagaReports>
								<source>${project.build.directory}/site</source>
							</sagaReports>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<id>jasmineserver</id>
			<build>
				<defaultGoal>jasmine:bdd</defaultGoal>
				<plugins>
					<plugin>
						<groupId>com.github.searls</groupId>
						<artifactId>jasmine-maven-plugin</artifactId>
						<version>1.3.1.6</version>
						<executions>
							<execution>
								<goals>
									<goal>bdd</goal>
								</goals>
							</execution>
						</executions>
						<configuration>
							<jsSrcDir>src/main/js</jsSrcDir>
							<jsTestSrcDir>src/test/js/jasmine</jsTestSrcDir>
							<debug>${debug}</debug>
							<preloadSources>
								<source>/src/main/webapp/lib/jquery-1.11.2.min.js</source>
								<source>/src/main/webapp/lib/jquery-ui-1.10.4.custom.min.js</source>
								<source>/src/test/js/lib/OpenLayers-test.js</source>
								<source>/src/test/js/lib/jasmin_util.js</source>
								<source>/src/main/webapp/js/config.js</source>
							</preloadSources>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
	</profiles>
	<build>
		<finalName>${proj.finalname}</finalName>
		<defaultGoal>install</defaultGoal>
		<plugins>
			<plugin>
				<groupId>com.github.klieber</groupId>
				<artifactId>phantomjs-maven-plugin</artifactId>
				<version>0.7</version>
				<executions>
					<execution>
						<goals>
							<goal>install</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<checkSystemPath>true</checkSystemPath>
					<version>${phantomjs.version}</version>
					<outputDirectory>${basedir}/cache</outputDirectory>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-release-plugin</artifactId>
				<version>2.5.2</version>
				<configuration>
					<pushChanges>false</pushChanges>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>2.7</version>
				<configuration>
					<encoding>${project.build.sourceEncoding}</encoding>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>buildnumber-maven-plugin</artifactId>
				<version>1.3</version>
				<executions>
					<execution>
						<phase>validate</phase>
						<goals>
							<goal>create</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<doCheck>false</doCheck>
					<doUpdate>false</doUpdate>
					<timestampFormat>{0,date,yyyy-MM-dd HH:mm:ssa}</timestampFormat>
					<shortRevisionLength>10</shortRevisionLength>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-antrun-plugin</artifactId>
				<version>1.8</version>
				<executions>
					<execution>
						<id>createDirecories</id>
						<phase>process-sources</phase>
						<goals>
							<goal>run</goal>
						</goals>
						<configuration>
							<target>
								<!-- YUI compressor heeft deze directory nodig -->
								<mkdir dir="${project.build.directory}/${project.build.finalName}/js" />
							</target>
						</configuration>
					</execution>
					<execution>
						<id>getYslow</id>
						<phase>prepare-package</phase>
						<goals>
							<goal>run</goal>
						</goals>
						<configuration>
							<target>
								<setproxy proxyhost="${http.proxyHost}"
										  proxyport="${http.proxyPort}" />
								<mkdir dir="${basedir}/cache" />
								<get src="http://yslow.org/yslow-phantomjs-3.1.8.zip"
									 dest="${basedir}/cache/"
									 usetimestamp="true" />
								<unzip src="${basedir}/cache/yslow-phantomjs-3.1.8.zip"
									   dest="${basedir}/target">
									<patternset>
										<include name="**/yslow.js" />
									</patternset>
								</unzip>
							</target>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>jaxb2-maven-plugin</artifactId>
				<version>2.1</version>
				<executions>
					<execution>
						<id>xjc</id>
						<goals>
							<goal>xjc</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<verbose>${debug}</verbose>
					<includeSchemasOutputPath>.</includeSchemasOutputPath>
					<noPackageLevelAnnotations>true</noPackageLevelAnnotations>
                    <target>2.0</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.3</version>
				<configuration>
					<debug>${debug}</debug>
					<optimize>!${debug}</optimize>
					<source>${jdk.version}</source>
					<target>${jdk.version}</target>
					<showDeprecation>${debug}</showDeprecation>
					<showWarnings>${debug}</showWarnings>
				</configuration>
			</plugin>
			<plugin>
				<groupId>nl.geodienstencentrum.maven</groupId>
				<artifactId>sass-maven-plugin</artifactId>
				<version>2.6</version>
				<executions>
					<execution>
						<phase>process-sources</phase>
						<goals>
							<goal>update-stylesheets</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<useCompass>true</useCompass>
					<resources>
						<resource>
							<source>
								<directory>src/main/sass/scss</directory>
								<includes>
									<include>*.scss</include>
								</includes>
							</source>
							<destination>${project.build.directory}/${project.build.finalName}/css</destination>
						</resource>
					</resources>
					<sassOptions>
						<always_update>true</always_update>
						<style>${sass.OutputStyle}</style>
						<debug_info>${debug}</debug_info>
					</sassOptions>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.18.1</version>
				<configuration>
					<excludes>
						<exclude>**/*IntegrationTest.java</exclude>
					</excludes>
					<!-- work around voor java 7. see:
					http://stackoverflow.com/questions/10869127/maven-build-causes-verifyerror-with-java-1-7
					-->
					<argLine>${surefire.argline}</argLine>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-failsafe-plugin</artifactId>
				<version>2.18.1</version>
				<configuration>
					<includes>
						<include>**/*IntegrationTest.java</include>
					</includes>
					<testFailureIgnore>true</testFailureIgnore>
					<systemPropertyVariables>
						<jetty.port>${jetty.port}</jetty.port>
					</systemPropertyVariables>
					<argLine>-XX:-UseSplitVerifier</argLine>
					<trimStackTrace>false</trimStackTrace>
				</configuration>
				<executions>
					<execution>
						<goals>
							<goal>integration-test</goal>
							<goal>verify</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.mortbay.jetty</groupId>
				<artifactId>jetty-maven-plugin</artifactId>
				<version>${jetty.version}</version>
				<configuration>
					<scanIntervalSeconds>10</scanIntervalSeconds>
					<stopPort>${jetty.stopPort}</stopPort>
					<stopKey>STOP</stopKey>
					<contextPath>/</contextPath>
					<connectors>
						<connector implementation="org.eclipse.jetty.server.bio.SocketConnector">
							<port>${jetty.port}</port>
							<maxIdleTime>60000</maxIdleTime>
						</connector>
					</connectors>
					<webAppConfig>
						<baseResource implementation="org.eclipse.jetty.util.resource.ResourceCollection">
							<resourcesAsCSV>src/main/webapp,target/cbsviewer</resourcesAsCSV>
						</baseResource>
						<sessionHandler implementation="org.eclipse.jetty.server.session.SessionHandler">
							<sessionManager implementation="org.eclipse.jetty.server.session.HashSessionManager">
								<usingCookies>false</usingCookies>
							</sessionManager>
						</sessionHandler>
					</webAppConfig>
				</configuration>
				<executions>
					<execution>
						<id>start-jetty</id>
						<phase>pre-integration-test</phase>
						<goals>
							<goal>run-war</goal>
						</goals>
						<configuration>
							<war>${project.build.directory}/${proj.finalname}.war</war>
							<scanIntervalSeconds>0</scanIntervalSeconds>
							<daemon>true</daemon>
						</configuration>
					</execution>
					<execution>
						<id>stop-jetty</id>
						<phase>post-integration-test</phase>
						<goals>
							<goal>stop</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.6</version>
				<configuration>
					<!-- niet kopieren -->
					<warSourceExcludes>**/css/*.css,**/js/*.js,**/lib/theme/default/style.css,**/jquery.fancybox.css,**/jquery.layout.js</warSourceExcludes>
					<archive>
						<forced>true</forced>
						<index>true</index>
						<manifest>
							<addDefaultImplementationEntries>true</addDefaultImplementationEntries>
							<addDefaultSpecificationEntries>true</addDefaultSpecificationEntries>
							<addClasspath>true</addClasspath>
						</manifest>
						<manifestEntries>
							<Implementation-Build>branch - ${scmBranch} rev - ${buildNumber} time - ${timestamp}</Implementation-Build>
							<SCM-Revision>${buildNumber}</SCM-Revision>
							<OpenLayers-Version>${openlayers.checkout.versiontype} - ${openlayers.checkout.version}</OpenLayers-Version>
							<url>${project.url}</url>
							<release>${project.version}</release>
						</manifestEntries>
					</archive>
				</configuration>
			</plugin>
			<plugin>
